# Copy this file to .env.local and update the values as needed

# Backend API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8080
NEXT_PUBLIC_API_VERSION=v1

# File Upload Configuration
NEXT_PUBLIC_MAX_FILE_SIZE=5242880
NEXT_PUBLIC_ALLOWED_FILE_TYPES=.pdf,.doc,.docx

# Application Configuration
NEXT_PUBLIC_APP_NAME=Super2025
NEXT_PUBLIC_APP_VERSION=1.0.0

# Company Information
NEXT_PUBLIC_COMPANY_EMAIL=careers@super2025.com
NEXT_PUBLIC_COMPANY_PHONE=+1-800-SUPER25

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG=true 